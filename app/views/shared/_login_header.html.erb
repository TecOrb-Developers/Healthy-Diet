<div style="float:right">
  <% if user_signed_in? -%>
	 <%= link_to 'Account', edit_user_registration_path, class: "btn btn-default", method: "get" %>
	 <%= link_to 'Log Out', destroy_user_session_path, class: "btn btn-default", method: "delete" %>
  <% else -%>
	 <%= link_to 'Sign Up', new_user_registration_path, class: "btn btn-default",  method: "get" %>
	 <%= link_to 'Log In', new_user_session_path, class: "btn btn-default",  method: "get" %>
  <% end -%>
</div>
